{
  "name": "cdk-modular-stacks",
  "version": "1.0.0",
  "description": "Example CDK project demonstrating modular stack architecture with shared constructs and configuration files",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context stage=dev",
    "deploy:staging": "cdk deploy --all --context stage=staging",
    "deploy:prod": "cdk deploy --all --context stage=prod",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "aws-cdk": "2.1023.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.208.0",
    "constructs": "^10.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "modular",
    "typescript"
  ],
  "author": "Scott Van Gilder",
  "license": "MIT"
}